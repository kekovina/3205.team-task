
{
    "ts-node": {
        "require": ["tsconfig-paths/register"]
    },
    "compilerOptions": {
        "target": "ES2016",
        "module": "commonjs",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "outDir": "./dist",
        "baseUrl": "./",
        "paths": {
            "@routes/*": ["./routes/*"],
            "@data/*": ["./data/*"],
            "@types": ["./types/index.ts"]
        }
    },

  }
  